/******************************************************************************

Description : sample code code complexity O(N^2)
Class : DIS1A
Date : 25/09/2024
Author : GOCL01
Institution : Fontys University of Applied Sciences

*******************************************************************************/
#include <iostream>

using namespace std;

int main()
{   int size = 2;
    int a[size][size] = {{1,2},{3,4}}; // Declare a 2D matrix (2x2)
    int b[size][size] = {{1,2},{3,4}}; // Declare a 2D matrix (2x2)
    int c[size][size];
    
    for(int i=0; i<size; i++)
        {for(int j=0; j<size; j++)
            c[i][j] = a[i][j]*b[i][j];}  // element-wise matrix multiplication
                                        // This is an example of O(N^2) complexity
        

    return 0;
}
